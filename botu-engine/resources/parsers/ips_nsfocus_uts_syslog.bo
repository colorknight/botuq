                OBJ_TYPE = "/Ip";
                OBJ_NAME = segs[15];
                OBJ_IP = segs[16];
                OBJ_MAC = convert2Mac(segs[18]);
            }
        case "DNS":
            pack("event"){
                START_TIME = extractTime(segs[20]);
                DS_DVC_NAME = segs[0];
                DS_PROCESS_NAME = segs[2];
                EVT_CATEGORY = segs[3];
                EVT_TYPE = segs[2];
                SEVERITY = 2;
