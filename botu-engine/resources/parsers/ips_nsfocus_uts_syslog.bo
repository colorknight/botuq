                SBJ_NAME = segs[9];
                SBJ_IP = segs[9];
                SBJ_PORT = segs[12];
                SBJ_MAC = convert2Mac(segs[11]);
                BHV_CATEGORY = "/Access";
                BEHAVIOR = segs[24];
                TRANS_PROTO = dictMapping("trans_protocol",segs[13]);
                OBJ_TYPE = "/Ip";
                OBJ_NAME = segs[15];
                OBJ_IP = segs[16];
                OBJ_MAC = convert2Mac(segs[18]);
                REQ_URL = segs[25];
                //OUTCOME = decodeBase64(segs[37]);
            }
        case "DBOP":
